            font-size: 1.2rem;
            margin-top: 20px;
            color: #f0f0f0;
        }

        .btn-primary {
            background-color: #FF6801;
            border: none;
            transition: transform 0.3s ease-in-out;
            font-weight: 600;
        }

        .btn-primary:hover {
            transform: scale(1.1);
            background-color: #e55c00;
        }

        /* About Section */
        .about-section {
            padding: 80px 0;
            background-color: #f7f7f7;
        }

        .about-img {
            transition: all 0.6s ease-in-out;
        }
        .about-img:hover {
            transform: scale(1.05) rotate(-2deg);
        }

        /* Services Section */
