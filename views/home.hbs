            transition: color 0.4s ease-in-out;
            font-weight: 600;
        }
        .navbar.scrolled .nav-link {
            color: #FF6801 !important;
        }
        .navbar-brand {
            color: #FF6801 !important;
            font-weight: bold;
            font-size: 1.8rem;
            background: linear-gradient(to right, #FF6801, #FFB677, #FF6801);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
