        body {
            font-family: 'Raleway', sans-serif;
            color: #303030;
            overflow-x: hidden;
        }

        /* Navbar */
        .navbar {
            background: transparent;
            transition: background 0.4s ease-in-out;
        }
        .navbar.scrolled {
            background-color: #045B2E; /* Green */
            box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1); 
